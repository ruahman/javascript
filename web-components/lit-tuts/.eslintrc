{
  "root":true,
  "parser":"@typescript-eslint/parser",
  "plugins":["@typescript-eslint", "prettier"], // show prettier formating errors
  "extends":[
     "eslint:recommended",
     "plugin:@typescript-eslint/eslint-recommended",
     "plugin:@typescript-eslint/recommended",   
     "prettier"
  ],
  "rules":{
    "no-console":1,// Means just warning   
    "prettier/prettier":2// Means error
  }
}
