{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:vue/essential", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "plugins": ["vue", "@typescript-eslint"],
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "no-unused-vars": "warn",
    "consistent-return": "off",
    "no-alert": "off"
  }
}
