  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Launch Deno",
      "type": "node",
      "program": "${workspaceFolder}/main.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "C:\\Users\\dego_\\.deno\\bin\\deno.EXE",
      "runtimeArgs": [
        "run",
        "--inspect-brk",
        "--allow-all",
        "--unstable"
      ],
      "attachSimplePort": 9229,
      "outputCapture": "std"
    }
  ]
}
